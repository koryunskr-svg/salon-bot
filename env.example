# .env.example
# Токен Telegram бота, полученный от @BotFather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# JSON-ключ сервисного аккаунта Google (вставьте строку целиком)
GOOGLE_CREDENTIALS_JSON={"type": "service_account", ...}

# ID вашей Google Таблицы (часть URL)
SHEET_ID=your_google_sheet_id_here

# ID вашего Google Календаря (часть URL)
CALENDAR_ID=your_google_calendar_id_here

# Часовой пояс (например, Europe/Moscow)
TZ=Europe/Moscow

# Таймаут резервирования слота (в секундах, по умолчанию 120)
RESERVATION_TIMEOUT=120

# Таймаут предупреждения о резервировании (в секундах, по умолчанию 60)
WARNING_TIMEOUT=60
